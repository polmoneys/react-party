        "@types/react": "^17.0.3",
        "node-sass": "^5.0.0",
        "postcss": "^8.2.10",
        "prettier": "^2.2.1",
        "rollup": "^2.45.2",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-postcss": "^4.0.0",
        "rollup-plugin-typescript2": "^0.30.0",
        "typescript": "^4.2.3"
    },
    "peerDependencies": {
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
